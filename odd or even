class HelloWorld {
    public static void main(String[] args) {
     int a[]={1,2,3,4,6,7,8,9};
     int i,j;
      System.out.println("even number:");
     for(i=0;i<a.length;i++){
     if(a[i]%2==0){
         System.out.println( a[i]);
        
        }
     }
     System.out.println("odd number :");

       for(j=0;j<a.length;j++) 
        {
        if(a[j]%2!=0){
          System.out.println(a[j]);
       }
    }
     }}
        
